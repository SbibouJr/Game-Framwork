{
  "source": "./src/",
  "destination": "./docs",
  "plugins": [
    {
      "name": "esdoc-standard-plugin",
      "option": {
        "lint": {"enable": true},
        "coverage": {"enable": true},
        "accessor": {"access": ["public", "protected"], "autoPrivate": true},
        "undocumentIdentifier": {"enable": true},
        "unexportedIdentifier": {"enable": false},
        "typeInference": {"enable": true},
        "brand": {
          "logo": "./src/assets/img/marsattakStudioGame.png",
          "title": "Sbjr-Game-Framwork",
          "description": "this is awesome library",
          "repository": "https://github.com/SbibouJr/Sbjr-Game-Framwork",
          "site": "https://sbibou-jr.fr",
          "author": "https://sbibou-jr.fr",
          "image": "./src/assets/img/marsattakStudioGame.png"
        },
        "test": {
          "source": "./src/",
          "interfaces": ["describe", "it", "context", "suite", "test"],
          "includes": ["(spec|Spec|test|Test)\\.js$"]
        }
      }
    }
  ]
}
